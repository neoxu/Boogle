<div class="container-fluid">
	<div class="row-fluid">
		<div class="span12">
		<form name="club" action="/club1" method="get">
		    <% for(var i=0; i< docs.length; i++){ %>
		    	<% if (docs[i].clubName && docs[i].backgroundimg && docs[i].backgroundimg.url)  { %>
				<div class="carousel slide" id="carousel-926043">
					<div class="carousel-inner">
						<div class="item active">
					   		<a href=/club1 title="聯盟詳細資料">
					   			<input type="hidden" name="clubname" value=<%= docs[i].clubName %> />
								<img alt="" src=<%=docs[i].backgroundimg.url%> />
								<div class="carousel-caption">
									<h4>
										<%= docs[i].clubName %>
									</h4>
									<p>
										<%= docs[i].clubInfo %>
									</p>							
								</div>
							</a>						
						</div>
				</div>
				<% } %>
			<% } %>
		</form>
		</div>
	</div>
</div>
